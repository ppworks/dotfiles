[core]
    editor = vim
    pager = less -R
    excludesfile = /Users/koshikawa/.gitignore
    compression = 0
[user]
    name = ppworks
    email = koshikawa@ppworks.jp
[color]
    ui = auto
[github]
    user = ppworks
[alias]
    a = add
    co = checkout
    st = status -s -b
    ll = log --graph --decorate --date=short --pretty
    ln = log --graph --decorate --date=short --pretty --name-status
    stat = log --stat --summary
    wh = whatchanged
    df = diff
    ds = diff --staged
    gr = grep -n
    rank = shortlog -s -n --no-merges
    amend = commit --amend
    ammend = amend
    sync = !git checkout master && git pull origin master && git branch --merged | grep -v master | xargs git branch -d && git fetch --prune
[rebase]
    autosquash = true
    autostash = true
[diff]
    algorithm = histogram
[gc]
    auto = 0
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[merge "mergepbx"]
	name = Xcode project files merger
	driver = mergepbx %O %A %B
[commit]
    template=/Users/koshikawa/.commit_template
[pager]
    branch=cat
    status=cat
